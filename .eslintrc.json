{
  "extends": [
    "standard",
    "prettier",
    "prettier/@typescript-eslint"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "rules": {
    "react/no-unescaped-entities": 0,
    "indent": ["error", 4],
    "no-tabs/no-tabs":"error",
    "@typescript-eslint/no-unused-vars": "error",
    "prettier/prettier": "error"
  }
}
